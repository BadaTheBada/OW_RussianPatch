		<zISOCode>ru</zISOCode>
		
		<!--singular: 1, 21, 31, 41, 51, 61, 71, 81, 101, 1001, ... яблоко-->
		<!--plural: 2~4, 22~24, 32~34, 42~44, 52~54, 62, 102, 1002, ... яблока-->
		<!--plural2: 0, 5~19, 100, 1000, 10000, 100000, 1000000, ... яблок-->
		<!--Requires simplifying-->
		<zSingularExpression><![CDATA[(x%10 == 1) AND (x%100 != 11)]]></zSingularExpression>
		<zPluralExpression><![CDATA[((x%10 >= 2) AND (x%10 <= 4) AND (x%100 != 12) AND (x%100 != 13) AND (x%100 != 14)) OR ((x%10 <= -2) AND (x%10 >= -4) AND (x%100 != -12) AND (x%100 != -13) AND (x%100 != -14))]]></zPluralExpression>
		<zThousandsSeparator>.</zThousandsSeparator>
		<zDecimalSeparator>,</zDecimalSeparator>
		<zCurrencyPrefix>$</zCurrencyPrefix>
		<zThousandPostfix>T</zThousandPostfix>
		<zMillionPostfix>M</zMillionPostfix>
